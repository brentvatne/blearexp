{
  "expo": {
    "name": "Blearexp",
    "description": "Make a cute blurry image background from any of your photos",
    "slug": "blearexp",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "version": "2.0.0",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "xyz.bront.blearexp",
      "icon": "./assets/icon-ios.png"
    },
    "android": {
      "icon": "./assets/icon-android.png",
      "package": "xyz.bront.blearexp",
      "permissions": ["READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE", "CAMERA"],
      "versionCode": 3
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "platforms": [
      "android",
      "ios",
      "web"
    ]
  }
}
